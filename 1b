getwd()
data1=read.delim("iris.csv")
r=head(data1,6)
variety=c("Iris-setosa","Iris-virginia","Iris-setosa","Iris-virginia","Iris-virginia","Iris-setosa")
r$Variety=variety
print(variety)
print(r$Variety)
print(r)

write.table(data1,file="file2.csv", sep= "\t", row.names=FALSE)

